{
  "expo": {
    "name": "PetHealth",
    "entryPoint": "app/inicial.js",
    "slug": "PetHealth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/app/imgs/splash.png",
    "userInterfaceStyle": "light",
    "scheme": "PetHealth",
    "splash": {
      "image": "./src/app/imgs/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/app/imgs/splash.png",
        "backgroundColor": "#fff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./src/app/imgs/splash.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
