{
  "expo": {
    "name": "GoDress",
    "entryPoint": "app/inicial.js",
    "slug": "GoDress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Logo.png",
    "userInterfaceStyle": "light",
    "scheme": "GoDress",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash.png",
        "backgroundColor": "#fff"
      }
    },
    "web": {
      "favicon": "./src/assets/Logo.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
